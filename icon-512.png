<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Fundo gradiente -->
  <defs>
    <linearGradient id="bgGrad512" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#001529;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#003a70;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#005eb8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="shipGrad512" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0080ff;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Fundo arredondado -->
  <rect width="512" height="512" rx="110" fill="url(#bgGrad512)"/>
  
  <!-- Círculo decorativo -->
  <circle cx="256" cy="256" r="180" fill="none" stroke="#00d4ff" stroke-width="3" opacity="0.2"/>
  
  <!-- Navio estilizado -->
  <g transform="translate(256, 256)" filter="url(#glow)">
    <!-- Casco principal -->
    <path d="M -130 50 L -100 -30 L 100 -30 L 130 50 Z" 
          fill="url(#shipGrad512)" stroke="#00e676" stroke-width="6"/>
    
    <!-- Detalhes do casco -->
    <rect x="-90" y="-20" width="180" height="60" rx="8" fill="#003a70" opacity="0.5"/>
    
    <!-- Cabine principal -->
    <rect x="-80" y="-80" width="160" height="50" rx="8" fill="#00e676"/>
    
    <!-- Torres -->
    <rect x="-60" y="-110" width="40" height="30" rx="5" fill="#00c853"/>
    <rect x="20" y="-110" width="40" height="30" rx="5" fill="#00c853"/>
    
    <!-- Chaminés -->
    <rect x="30" y="-135" width="30" height="25" rx="4" fill="#ff6633"/>
    <rect x="-60" y="-145" width="25" height="35" rx="4" fill="#ff6633"/>
    
    <!-- Fumaça -->
    <circle cx="-47" cy="-160" r="8" fill="#fff" opacity="0.4"/>
    <circle cx="-52" cy="-175" r="10" fill="#fff" opacity="0.3"/>
    <circle cx="45" cy="-150" r="7" fill="#fff" opacity="0.4"/>
    
    <!-- Âncora -->
    <path d="M -110 10 L -110 30 M -110 20 L -120 30 M -110 20 L -100 30" 
          stroke="#ffaa00" stroke-width="4" fill="none"/>
    
    <!-- Ondas -->
    <path d="M -180 70 Q -150 60 -120 70 Q -90 80 -60 70 Q -30 60 0 70 Q 30 80 60 70 Q 90 60 120 70 Q 150 80 180 70" 
          stroke="#00d4ff" stroke-width="5" fill="none" opacity="0.7"/>
    <path d="M -180 90 Q -150 80 -120 90 Q -90 100 -60 90 Q -30 80 0 90 Q 30 100 60 90 Q 90 80 120 90 Q 150 100 180 90" 
          stroke="#00d4ff" stroke-width="4" fill="none" opacity="0.5"/>
    <path d="M -180 110 Q -150 100 -120 110 Q -90 120 -60 110 Q -30 100 0 110 Q 30 120 60 110 Q 90 100 120 110 Q 150 120 180 110" 
          stroke="#00d4ff" stroke-width="3" fill="none" opacity="0.3"/>
  </g>
  
  <!-- Texto BRAM -->
  <text x="256" y="450" font-family="Arial, sans-serif" font-size="64" font-weight="bold" 
        fill="#00d4ff" text-anchor="middle" filter="url(#glow)">BRAM</text>
  <text x="256" y="485" font-family="Arial, sans-serif" font-size="28" font-weight="normal" 
        fill="#00e676" text-anchor="middle" opacity="0.9">OFFSHORE</text>
</svg>
